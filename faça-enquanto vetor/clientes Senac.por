programa  
{  
    funcao inicio()  
    {  
         
        inteiro idades[5], i = 0
        cadeia nomes[5]

        faca{
          escreva("Digite o seu nome: ")
          leia(nomes[i])

          escreva("Digite a sua idade: ")
          leia(idades[i])
          i++
          }enquanto(i<5) 

          i=0
          faca{
          escreva("Aluno: " , nomes[i] ," idade: ", idades[i], "\n")
          i++
          }enquanto(i<5)

    }
}